{
  "name": "reactest",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.1.0",
    "cross-fetch": "^2.1.0",
    "es6-promise-polyfill": "^1.2.0",
    "node-sass-chokidar": "^1.2.2",
    "open-iconic": "^1.1.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-loading": "^1.0.4",
    "react-loading-bar": "0.0.7",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.0.3",
    "react-router": "^4.2.0",
    "react-scripts": "1.1.4",
    "react-scripts-sass": "^1.1.3",
    "react-spinners": "^0.3.2",
    "react-x-editable": "v0.0.9-beta",
    "reactstrap": "^5.0.0",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "sass:watch": "sass --watch src/styles/scss:src/styles/css",
    "sass:build": "sass --update src/styles/scss:src/styles/css",
    "build": "yarn sass:build && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start": "concurrently --kill-others \"python dummyserver.py\" \"react-scripts start\""
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "react-open-iconic-svg": "^2.0.0",
    "sass-loader": "^7.0.1"
  },
  "proxy": {
    "/app": {
      "target": "http://localhost:3001",
      "ws": false
    }
  }
}
